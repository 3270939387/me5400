def Scope "Looks"
{
    def Material "OmniPBR"
    {
        token outputs:mdl:displacement.connect = </Looks/OmniPBR/Shader.outputs:out>
        token outputs:mdl:surface.connect = </Looks/OmniPBR/Shader.outputs:out>
        token outputs:mdl:volume.connect = </Looks/OmniPBR/Shader.outputs:out>

        def Shader "Shader"
        {
            uniform token info:implementationSource = "sourceAsset"
            uniform asset info:mdl:sourceAsset = @OmniPBR.mdl@
            uniform token info:mdl:sourceAsset:subIdentifier = "OmniPBR"
            color3f inputs:diffuse_color_constant = (2, 0.2, 0.2)
            token outputs:out (
                renderType = "material"
            )
        }
    }
}
